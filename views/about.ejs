</section>

<section class="fetaure-page ">
	<div class="container">
		<div class="row">
			<% if(dept.length> 0 ) {
				for(let data of dept)
				{
					if(data.status)
					{
				%>
			<div class="col-lg-3 col-md-6">
				<div class="about-block-item mb-5 mb-lg-0">
					<img src="uploads/<%=data.dept_image%>" alt="" class="img-fluid w-100">
					<h4 class="mt-3"><%= data.dept_name%></h4>
					<p><%= data.dept_description%></p>
				</div>
			</div>
			<%}}}%>
		</div>
	</div>
</section>
